<!-- Create a database of a student and validate with the help of DTD (Document TYpe Definition) using XMl. -->

<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE student [

<!ELEMENT student (name,branch,yoj,email)>
<!ELEMENT name(#PCDATA)>
<!ELEMENT branch(#PCDATA)>
<!ELEMENT yoj(#PCDATA)>
<!ELEMENT email(#PCDATA)>
<!ENTITY writer " tripti">
<!ENTITY copyrights "PM">
<!ENTITY nbsp "&#xA0;">
<!ATTLIST student name CDATA #REQUIRED>
<!ATTLIST student branch CDATA #REQUIRED>
<!ATTLIST student yoj CDATA #REQUIRED>
<!ATTLIST student email CDATA #IMPLIED>

]>

